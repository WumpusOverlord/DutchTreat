<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Dutch Treat: @ViewBag.Title</title>
    <link rel="stylesheet" href="~/node_modules/font-awesome/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/node_modules/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>




<body>
<div class="navbar navbar-inverse navbar-fixed-top">
    <header class="container">
        <div class="navbar-header">
            <div class="navbar-brand"><i class="glyphicon glyphicon-apple"></i> Dutch Treat</div>
            <button class="navbar-toggle" data-toggle="collapse" data-target='#menu'></button>
        </div>
        <div id="menu" class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li><a asp-controller="App" asp-action="Index">Home</a></li>
                <li><a asp-controller="App" asp-action="Contact">Contact</a></li>
                <li><a asp-controller="App" asp-action="About">About</a></li>
      
            </ul>
        </div>
    </header>
</div>
<section class="container">
    <h2>@ViewBag.Title</h2>
    @RenderBody()
</section>
<footer class="container">
    <div class="text-center">Copyright 2017 Dutch Treat LLC</div>
</footer>

<script src="~/node_modules/jquery/dist/jquery.min.js"></script>
<script src="~/node_modules/bootstrap/dist/js/bootstrap.min.js"></script>
<script src="~/js/site.js"></script>
@RenderSection("Scripts", required: false)

</body>
</html>
